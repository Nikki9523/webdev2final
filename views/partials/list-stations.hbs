{{#each stations}}
<div class="box box-link-hover-shadow">
  <h2 class="title">
    {{title}} <br>
  </h2>
  <h4>
    Latitude: {{latitude}} <br>
    Longitude: {{longitude}}
    MinTemp: {{minTemp}}
  </h4>
  <a href="/station/{{_id}}" class="button">
    {{> icons/folder-icon}}
  </a>
  </h2>
</div>
{{/each}}