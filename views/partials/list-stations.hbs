{{#each stations}}
<div class="box box-link-hover-shadow">
  <h2 class="title">
    {{title}} <br>
  </h2>
  <h4>
   <b> Latitude: </b> {{latitude}} <br>
   <b>Longitude:</b>  {{longitude}}
  </h4>
  <a href="/station/{{_id}}" class="button">
    {{> icons/folder-icon}}
  </a>
    <a href="/dashboard/deletestation/{{_id}}" class="button">
    {{> icons/delete-icon}}
  </a>
  </h2>
</div>
{{/each}}